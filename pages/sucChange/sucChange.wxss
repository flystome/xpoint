/* pages/sucChange/sucChange.wxss */
.container {
  display: block;
}
.sucInfo {
  width: 100%;
  text-align: center;
  padding-top: 48rpx;
}
.sucInfo image {
  width: 136rpx;
  height: 136rpx;
  border-radius: 68rpx;
}
.sucInfo text {
  display: block;
  font-size: 36rpx;
  line-height: 50rpx;
  color: #212A42;
  margin-top: 24rpx;
}
.content {
  margin: 0 34rpx;
  padding: 56rpx 0 30rpx;
  display: flex;
  border-bottom: 1px solid #ECECEC;
}
.content>image {
  width: 136rpx;
  height: 136rpx;
  border-radius: 68rpx;
  margin-right: 30rpx;
}
.content view {
  flex: 1;
}
.content .name {
  font-size: 32rpx;
  line-height: 45rpx;
  color: #212A42;
  display: block;
  margin-bottom: 12rpx;
}
.name text {
  float: right;
}
.content .desc {
  display: block;
  font-size: 24rpx;
  line-height: 32rpx;
  color: #5E6A88;
  width: 84%;
}
.total {
  padding: 32rpx 34rpx 0;
  display: flex;
  height: 56rpx;
  line-height: 56rpx;
  font-size: 40rpx;
  color: #212A42;
}
.total view {
  flex: 1;
  font-size: 32rpx;
  text-transform: capitalize;
  height: 56rpx;
  line-height: 56rpx;
}
.total view image {
  width: 56rpx;
  height: 56rpx;
  border-radius: 28rpx;
  margin-right: 16rpx;
  vertical-align: -14rpx;
}
.back {
  height: 88rpx;
  width: 70%;
  border-radius: 44rpx;
  background: #0BBD89;
  color: #ffffff;
  font-size: 36rpx;
  margin-top: 80rpx;
}
.go {
  text-align: center;
  display: block;
  margin-top: 40rpx;
  font-size: 28rpx;
  color: #0BBD89;
}